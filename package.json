    "test": "export $(cat .env.test.local | xargs) && playwright test",
    "test:debug": "PWDEBUG=1 pnpm t -- --headed",
    "test:ci": "playwright test"
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": [
      "pnpm format"
